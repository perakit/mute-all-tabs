{
    "manifest_version": 3,
    "name": "Mute All Tabs",
    "version": "1.0",
    "permissions": [
        "tabs"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Mute All Tabs"
    }
}